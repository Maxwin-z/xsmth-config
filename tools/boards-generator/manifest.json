{
  "manifest_version": 2,

  "name": "boards-generator",
  "description": "xx",
  "version": "1.0",

  "permissions": [
    "https://m.newsmth.net/*", "tabs"
  ],

  "content_scripts": [
    {
      "matches": ["http://m.newsmth.net/*"],
      "js": ["board.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
